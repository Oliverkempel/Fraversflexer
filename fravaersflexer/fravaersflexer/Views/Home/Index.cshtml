@using fravaersflexer.Models;
@model IEnumerable<Person>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


<table class="bg-white">
      <thead>
    <tr>
      <th>Navn</th>
      <th>Alder</th>
      <th>Skole</th>
        <th>Uddanelse</th>
      <th>Klasse</th>
      <th>Fraværs Procnet</th>
    </tr>
      </thead>
        <tbody>
    @foreach (Person persons in Model)
    {
        <tr>
            <td>@persons.Name</td>
            <td>@persons.Age</td>
            <td>@persons.SchoolName</td>
            <td>@persons.EducationName</td>
            <td>@persons.ClassName</td>
            <td>@persons.AbsencePercentage</td>
        </tr>
            }
    </tbody>
</table>